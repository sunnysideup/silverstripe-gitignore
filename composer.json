{
    "name": "sunnysideup/gitignore",
    "description": "No more manual editing for .gitignore - adds all the basics in one go",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "gitignore",
        "dev tool",
        "setup",
        "install",
        ".gitignore"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0 || ^6.0"
    },
    "require-dev": {
        "sunnysideup/sswebpack_engine_only": "dev-master",
        "silverleague/ideannotator": "*",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\GitIgnore\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ]
}